  "license": "MIT",
  "dependencies": {
    "express": "^4.13.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "cheerio": "^0.19.0",
    "request": "^2.67.0",
    "ytdl-core": "^0.7.9"
  },
  "devDependencies": {
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "livereloadx": "^0.3.9"
  }
}
